    "class.extend": "0.9.2",
    "coveralls": "3.0.2",
    "eslint": "6.8.0",
    "expect.js": "0.3.1",
    "istanbul": "0.4.5",
    "jwt-simple": "0.5.1",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "q": "1.4.1",
    "rewire": "2.5.2",
